<h1>Rock's Best Dancers Members</h1>
<table data-toggle="table"
       data-url="/members/rbd"
       data-cache="false"
       data-classes="table table-hover"
       data-striped="true"
       data-sort-name="bboName"
       data-sort-order="asc"
       data-search="true"
       data-pagination="true"
       data-page-size="10"
       data-page-list="[10, 25, 50, 100]"
       data-smart-display="true"
       data-side-pagination="server">
  <thead>
    <th data-field="bboName" data-sortable="true">BBO login</th>
    <th data-field="nation" data-sortable="true">Country</th>
    <th data-field="level" data-sortable="true">Level</th>
    <th data-field="rbdAwards" data-sortable="true">Awards</th>
    <th data-field="rbdAverageScore" data-sortable="true" data-formatter="bsTable.formatters.float"><span class="average">IMPs</span></th>
    <th data-field="rbdNumTournaments" data-sortable="true"># Tournaments</th>
  </thead>
</table>
