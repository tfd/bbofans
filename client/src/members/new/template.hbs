<div class="modal-header">
  <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
  <h1 class="modal-title">Add new member</h1>
</div>

<div class="modal-body" id="blacklist-form">
  <form role="form">

    <div class="form-group col-md-4">
      <label for="new-bboName" class="control-label">BBO Name</label>
      <input type="text" id="new-bboName" name="bboName" class="typeahead form-control" />
    </div>
    <div class="clearfix"></div>

    <div class="form-group col-md-12 text-right">
      <button class="btn form-cancel">Cancel</button>
      <button class="btn btn-default btn-primary form-submit">Create</button>
    </div>
    <div class="clearfix"></div>

  </form>
</div>
